//courses effect
$(".course1").on({
	mouseenter: function(){
        $(".fa-qrcode").css({"text-shadow": "2px 2px 6px #999", "transform": "scale(1.5,1.5)", "transition": "0.5s all"});
    },
    mouseleave: function(){
        $(".fa-qrcode").css({"text-shadow": "none", "transform": "rotateY(0deg)", "transition": "0.5s all"});
    } 
});
